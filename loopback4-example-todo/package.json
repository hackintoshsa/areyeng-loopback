{
  "name": "areyeng-carpool",
  "description": "Backend API for Areyeng Carpool App using LoopBack 4, MongoDB, GraphQL, Twilio/Nexmo/MSG91 for OTP, SendGrid for email, and IBM Watson for personality matching",
  "version": "1.0.0",
  "keywords": [
    "loopback",
    "carpool",
    "IBM Watson",
    "GraphQL",
    "MongoDB",
    "Twilio",
    "Nexmo",
    "SendGrid",
    "OTP",
    "personality"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "HackintoshSA Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/hackintoshsa/areyeng-carpool.git"
  },
  "engines": {
    "node": "18 || 20 || 22"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo package",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "verify": "npm pack && tar xf areyeng-carpool*.tgz && tree package && npm run clean",
    "premigrate": "npm run build ",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node .",
    "start:watch": "tsc-watch --target es2017 --outDir ./areyeng-dist --onSuccess \"npm start\""
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@loopback/authentication": "^11.0.7",
    "@loopback/authentication-jwt": "^0.15.7",
    "@loopback/boot": "^7.0.7",
    "@loopback/core": "^6.1.4",
    "@loopback/repository": "^7.0.7",
    "@loopback/rest": "^14.0.7",
    "@loopback/rest-explorer": "^7.0.7",
    "@loopback/service-proxy": "^7.0.7",
    "@sendgrid/mail": "^8.1.4",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "log4js": "^6.9.1",
    "loopback-connector-mongodb": "^6.2.0",
    "loopback-connector-rest": "^5.0.1",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "msg91": "2.2.4",
    "nexmo": "^2.2.2",
    "nodemailer": "^6.9.16",
    "tsc-watch": "^6.2.0",
    "tslib": "^2.6.3",
    "twilio": "^3.80.0"
  },
  "devDependencies": {
    "@loopback/build": "^11.0.6",
    "@loopback/eslint-config": "^15.0.4",
    "@loopback/http-caching-proxy": "^6.0.7",
    "@loopback/testlab": "^7.0.6",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^16.18.101",
    "@types/nodemailer": "^6.4.16",
    "eslint": "^8.57.0",
    "lodash": "^4.17.21",
    "typescript": "~5.2.2"
  },
  "gitHead": "5bca4d1d6af77cd5ea8fdecf55b11b939efbf217"
}
